    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-unused-imports": "^4.0.1",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
