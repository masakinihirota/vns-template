    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-react": "0.330.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-merge-refs": "^2.1.1",
    "stripe": "^14.25.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
